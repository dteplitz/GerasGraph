# Configuración del Agente LangGraph con Groq

# API Key de Groq (obligatorio)
# Obtén tu key en: https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# Modelo de Groq a usar (opcional)
# Opciones: llama3-8b-8192, llama3-70b-8192, mixtral-8x7b-32768, gemma2-9b-it
GROQ_MODEL=llama3-8b-8192

# Temperatura del modelo (opcional)
# Valores: 0.0 (muy determinístico) a 1.0 (muy creativo)
GROQ_TEMPERATURE=0.1

# Ruta de la base de datos SQLite (opcional)
DB_PATH=agent_memory.db

# ID de la sesión (opcional)
SESSION_ID=user_session_1

OPENAI_API_KEY=
OPENAI_MODEL=gpt-4.1-mini
OPENAI_VECTOR_STORE_ID=